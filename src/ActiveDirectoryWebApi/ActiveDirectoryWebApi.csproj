	<PackageReference Include="System.DirectoryServices.Protocols" Version="9.0.11" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ActiveDirectoryWeb.ServiceDefaults\ActiveDirectoryWeb.ServiceDefaults.csproj" />
  </ItemGroup>

</Project>
